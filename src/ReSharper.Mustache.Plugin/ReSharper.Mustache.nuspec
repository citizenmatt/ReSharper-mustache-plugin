<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ReSharper.Mustache</id>
    <title>Mustache MVC support</title>
    <version>1.0.0.11</version>
    <authors>Tim Schmidt</authors>
    <owners>Tim Schmidt</owners>
    <description>Allows ReSharper to locate .mustache files as MVC views</description>
    <projectUrl>https://github.com/taschmidt/ReSharper-mustache-plugin</projectUrl>
    <copyright>Copyright &#169; 2013 Tim Schmidt</copyright>
    <dependencies>
      <dependency id="ReSharper" version="8.0" />
    </dependencies>
    <tags>resharper mustache nustache view engine</tags>
  </metadata>
  <files>
    <!-- 8.0 -->
    <file src="bin\Release\ReSharper.Mustache.Plugin.8.0.dll"
          target="ReSharper\v8.0\plugins" />

    <!-- 8.1 is much more declarative! -->
    <file src="Nustache.Core.ExternalAnnotations.xml" 
          target="ReSharper\v8.1\annotations\Nustache.Core.xml" />
  </files>
</package>